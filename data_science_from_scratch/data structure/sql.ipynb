   "source": [
    "#### find the second largest\n",
    "+----+--------+\n",
    "| Id | Salary |\n",
    "+----+--------+\n",
    "| 1  | 100    |\n",
    "| 2  | 200    |\n",
    "| 3  | 300    |\n",
    "+----+--------+"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "```\n",
    "SELECT MAX(Salary) \n",
    "FROM Employee \n",
    "WHERE Salary < (SELECT MAX(Salary) FROM Employee);\n",
    "```"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "```\n",
    "SELECT Salary \n",
    "FROM (SELECT Salary FROM Employee ORDER BY Salary DESC LIMIT 2)\n",
    "ORDER BY Salary\n",
    "LIMIT 1;\n",
    "```"
   ]
  }
 ],
